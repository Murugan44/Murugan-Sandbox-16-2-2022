<apex:page controller="VG_Revise_SAPIProductSteps" sidebar="false" showHeader="false">
  <head>
    <style>
        .disabled
        {
          border: 1px solid #999999;
          background-color: #dddddd;
          color: #666666;
          opacity: 0.65; 
          cursor: not-allowed;
        }
    </style>
    <script type="text/Javascript">
        function SubmitOnClick (objSubmitBtn) {
            
            document.getElementById(objSubmitBtn.id).disabled = true;
            document.getElementById(objSubmitBtn.id).classList.add("disabled");
            objSubmitBtn.value = "Savingâ€¦";
            doSubmit();
        }
    </script>
</head>  
    <apex:form style="width:1300px;"> 
        <apex:actionStatus id="entryStatus">
           <apex:facet name="start">
               <div id="loadingDiv" style="background-color:black; opacity:0.5; height:4000px; width:100%; z-index:1; position: absolute;">
                  <img src="{!$Resource.Loading}" height="125px" style="position: fixed; top: 50%; left: 50%; transform: translate(-50%, -50%);"/>
              </div>
           </apex:facet>
        </apex:actionStatus>
        
        <div style="width: 80%; margin-bottom:8px;margin-top: 8px;margin-left:140px;">
            <img src="{!$Resource.LS_Finish}" alt="Smiley face" height="45px" width="800px" style="margin-left: 15px;margin-top: 15px;margin-bottom:10px;"/>
        </div>   
        <apex:pageBlock title="Summary" tabStyle="Account" id="pg1">
           <apex:pageMessages />
           <apex:pageBlockButtons location="Top">
                <apex:commandButton action="{!Cancel}" value="Cancel" onclick="if(!confirm('Are you sure?')){return false;}"/>
               <apex:commandButton value="Previous" rendered="{!(MTAType  !=  'Bad Debt Provision' && ps.VG_MTA_Type__c != 'Bad Debt Provision')}" action="{!gotoStep5b}"/>
               <!--<apex:commandButton rendered="{!(MTAType  !=  'Bad Debt Provision' && ps.VG_MTA_Type__c != 'Bad Debt Provision')}" value="Previous" action="{!gotoStep2}"/>-->
                <apex:commandButton value="Finish" id="Submit" action="{!Finish}" status="entryStatus" rerender="pg1"/>
            </apex:pageBlockButtons>
            
            <apex:pageBlockSection columns="2">                             
                  <apex:pageBlockSectionItem rendered="{!(MTAType =='Run-Off' || MTAType =='Revision of Cover' || MTAType == 'Reinstatement Premium') && seletedProrateoption == 'Flat'}">
                        <apex:outputLabel >Risk Premium</apex:outputLabel>
                        <apex:inputText value="{!RiskPremium}" Style="Width:197px;"><apex:actionSupport event="onchange" action="{!UpdateFinalCalculations}"  reRender="pg1"  status="entryStatus"/></apex:inputText>
                    </apex:pageBlockSectionItem>
                    <apex:pageBlockSectionItem rendered="{!(MTAType =='Run-Off' || MTAType =='Revision of Cover' || MTAType == 'Reinstatement Premium') && seletedProrateoption != 'Flat'}">
                        <apex:outputLabel >Risk Premium</apex:outputLabel>
                        <apex:outputText value="{0, number,###,##0.00}"><apex:param value="{!RiskPremium}"/></apex:outputText>
                    </apex:pageBlockSectionItem>
                    <apex:pageBlockSectionItem rendered="{!(MTAType == null || MTAType == 'Bad Debt Provision' || MTAType == 'Run-off') && ps.Seleted_Prorate_Option__c != 'Flat'}">
                        <apex:outputLabel >Risk Premium </apex:outputLabel>
                        <apex:outputText value="{0, number,###,##0.00}"><apex:param value="{!RiskPremium}"/></apex:outputText>
                    </apex:pageBlockSectionItem>
                    <apex:pageBlockSectionItem rendered="{!ps.Seleted_Prorate_Option__c == 'Flat' && seletedProrateoption == null}">
                        <apex:outputLabel >Risk Premium</apex:outputLabel>
                        <apex:inputText value="{!RiskPremium}" Style="Width:197px;"><apex:actionSupport event="onchange" action="{!UpdateFinalCalculations}"  reRender="pg1"  status="entryStatus"/></apex:inputText>
                    </apex:pageBlockSectionItem>
                
                <apex:pageBlockSectionItem >
                    <apex:outputLabel >Written Share (%)</apex:outputLabel>                        
                    <apex:outputPanel styleClass="requiredInput" layout="block" id="pwPanel2" >
                    <apex:outputPanel styleClass="requiredBlock"/>       
                        <apex:inputText value="{!writtenShare}" style="width:197px;"><apex:actionSupport event="onchange" action="{!UpdateFinalCalculations}" status="entryStatus"  reRender="pg1"/></apex:inputText>
                    </apex:outputPanel>
                </apex:pageBlockSectionItem>
                       
                <apex:pageBlockSectionItem rendered="{!(MTAType =='Run-Off' || MTAType =='Revision of Cover' || MTAType == 'Reinstatement Premium') && seletedProrateoption == 'Flat'}">
                    <apex:outputLabel >Endorsement Premium</apex:outputLabel>
                    <apex:inputText value="{!EndorsementPremium}" Style="Width:197px;"><apex:actionSupport event="onchange" action="{!UpdateFinalCalculations}"  reRender="pg1" status="entryStatus"/></apex:inputText>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem rendered="{!(MTAType =='Run-Off' || MTAType =='Revision of Cover' || MTAType == 'Reinstatement Premium') && seletedProrateoption != 'Flat'}">
                    <apex:outputLabel >Endorsement Premium</apex:outputLabel>
                    <apex:outputText value="{0, number,###,##0.00}"><apex:param value="{!EndorsementPremium}"/></apex:outputText>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem rendered="{!(MTAType == null || MTAType == 'Bad Debt Provision' || MTAType == 'Run-off') && ps.Seleted_Prorate_Option__c != 'Flat'}">
                    <apex:outputLabel >Endorsement Premium</apex:outputLabel>
                    <apex:outputText value="{0, number,###,##0.00}"><apex:param value="{!EndorsementPremium}"/></apex:outputText>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem rendered="{!ps.Seleted_Prorate_Option__c == 'Flat' && seletedProrateoption == null}">
                    <apex:outputLabel >Endorsement Premium</apex:outputLabel>
                    <apex:inputText value="{!EndorsementPremium}" Style="Width:197px;"><apex:actionSupport event="onchange" action="{!UpdateFinalCalculations}"  reRender="pg1" status="entryStatus"/></apex:inputText>
                </apex:pageBlockSectionItem> 
                <apex:pageBlockSectionItem >
                    <apex:outputLabel >Signed Share (%)</apex:outputLabel>
                    <apex:outputPanel styleClass="requiredInput" layout="block" id="pwPanel2" >
                    <apex:outputPanel styleClass="requiredBlock"/>   
