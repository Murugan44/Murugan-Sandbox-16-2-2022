                        <apex:selectOption itemValue="--None--" itemLabel="--None--"/>
                        <apex:selectOptions value="{!NonEEAVolanteEntity_Select}"></apex:selectOptions>
                    </apex:selectList>
                </apex:pageBlockSectionItem>
                <apex:pageBlockSectionItem rendered="{!!ps.VG_Entity_Split_Required__c && (ps.Split_Type__c == 'EEA') && (MTAType  !=  'Bad Debt Provision' && ps.VG_MTA_Type__c != 'Bad Debt Provision')}">
